{
  "name": "modular-smart-account-pay",
  "version": "0.0.1",
  "scripts": {
    "prettier:sol": "prettier --write --plugin=prettier-plugin-solidity 'contracts/**/*.sol'",
    "prettier:solhint": "solhint 'contracts/**/*.sol' --fix --noPrompt",
    "lint:ts": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "lint:ts:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "docgen": "hardhat docgen",
    "prepare": "husky",
    "sendtx:xlayer": "NODE_ENV=dev npx hardhat run scripts/sendUopBatch.ts --network xlayer",
    "test:dev": "NODE_ENV=dev npx hardhat test",
    "test:local": "NODE_ENV=local npx hardhat test --network local",
    "test:paymaster": "NODE_ENV=dev npx hardhat test test/paymaster/*",
    "deploy:paymaster:dev": "NODE_ENV=dev npx hardhat run scripts/deploy/deployPaymaster.ts --network network",
    "deploy:paymaster:prod": "npx hardhat run scripts/deploy/deployPaymaster.ts --network network",
    "coverage": "NODE_ENV=dev npx hardhat coverage --testfiles 'test/**/*.ts'",
    "senduop:local": "npx hardhat run scripts/sendUopBatchPayERC20.ts --network local",
    "deploy": "npx hardhat run scripts/deploy/deploy.ts --network local",
    "gobind": "npx hardhat gobind",
    "loadtest": "npx hardhat run scripts/loadtest.ts --network local"
  },
  "dependencies": {
    "@account-abstraction/contracts": "0.7.0",
    "@openzeppelin/contracts": "5.1.0",
    "@openzeppelin/contracts-upgradeable": "5.1.0",
    "@peculiar/asn1-ecc": "^2.3.13",
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@solarity/hardhat-gobind": "^1.2.2",
    "ec-pem": "^0.18.0",
    "elliptic": "^6.5.7",
    "solady": "0.0.234",
    "solidity-linked-list": "6.2.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-ignition": "^0.15.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.9",
    "@nomicfoundation/ignition-core": "^0.15.5",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/hardhat-upgrades": "^3.2.1",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.17",
    "@types/mocha": "^10.0.7",
    "@uniswap/swap-router-contracts": "^1.3.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "chai": "^4.5.0",
    "cloc": "^2.2.0-cloc",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "ethers": "^6.13.2",
    "globals": "^15.9.0",
    "hardhat": "^2.22.10",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^2.2.2",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "prettier-plugin-solidity": "^1.4.1",
    "solidity-coverage": "^0.8.14",
    "solidity-docgen": "^0.6.0-beta.36",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "web3": "^1.10.4"
  },
  "lint-staged": {
    "*.sol": [
      "prettier --write --plugin=prettier-plugin-solidity 'contracts/**/*.sol'"
    ]
  }
}
